<% layout('./public/layout') %>
    <div class="row">

        <div class="col-lg-6 col-md-offset-3">
            <form method="post" action="/addseller">
                <h1>Agregar Vendedor </h1>

                <div class="form-group">
                    <label> Nombre </label>
                    <input type="text" name="name" class="form-control" required="true" />
                </div>


<div class="form-group">
                                    <label> Apellido </label>
                                    <input type="text" name="lastname" class="form-control" required="true" />
  </div>


  <div class="form-group">
                                      <label> Identificacion </label>
                                      <input type="text" name="ident" class="form-control" required="true" />
    </div>


      <div class="form-group">
                                          <label> Comision % </label>
                                          <input type="number" name="comission" class="form-control" required="true" />
        </div>



  <div class="form-group">
      <label> Nombre Usuario </label>
      <input type="text" name="username" class="form-control" required="true" />
  </div>


<div class="form-group">
                      <label> Password </label>
                      <input type="password" name="password" class="form-control" required="true" />
</div>




<div class="container-fluid">
                <button type="submit" class="btn btn-success">Agregar Vendedor </button>
                <a href="#menu-toggle" class="btn btn-default" id="menu-toggle">Ver Menu</a></div>
            </div>
            </form>

        </div>
    </div>
